  <style>
    @import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;800&display=swap');
    @font-face {
    font-family: 'Pretendard-Regular';
    src: url('https://cdn.jsdelivr.net/gh/Project-Noonnu/noonfonts_2107@1.1/Pretendard-Regular.woff') format('woff');
    font-weight: 400;
    font-style: normal;
    }
    * {
      font-family: 'Pretendard-Regular';
    }
    </style>

</head>
<body>
    <div class="container" id="header">
      <div id="logo">
        <a href="/main">
          <img src="./img/logo.jpg" alt="logo">
        </a>
      </div>

      <div id="menubar" >
        <ul>
          <li><a href="/main" style="color:#C7C7C7"> HOME </a></li>
          <li><a href="/product"> ESHOP </a></li>
          <li><a href="/regist" > Trade-Regist </a></li>
          <li><a href="about.html"> ABOUT </a></li>
          <li><a href="/notice"> NOTICE </a></li>
          <li><a href="/mypage"> <span class="glyphicon glyphicon-user"></span></a></li>
        </ul>
      </div>

    </div>

    <form action="/regist/create" method="POST" enctype="multipart/form-data">
    <div class="container form-group">

      <div class="text-left" style="padding:30px 0px 30px 0px">
        <h2>Notice</h2>


        

   


        <div class="row" id="regist-img" style="padding:30px 0px 30px 0px; border-top:3px solid black; border-bottom:0.5px solid #C7C7C7;">
          <table class="table table-striped">
            <thead>
            <th>No.</th>
            <th>Title</th>
            <th>Uploader</th>
            <th>Date</th>
          </thead>
            <tr>
              <td class="col-md-2">공지번호</td>
              <td class="col-md-6">공지제목</td>
              <td>공지 작성자</td>
              <td>공지 게시일</td>
            </tr>
      
            <% for(var i=0; i<notice.length; i++){ %>
                <tr>
                    <td><%= notice[i].noticeId %></td>
                    <td><%= notice[i].noticeTitle %></td>
                    <td><%= notice[i].nickname %></td>
                    <td><%= notice[i].noticeTime %></td>
                  </tr>

                <% } %>
          </table>

       
  

            </div>

          </div>
        </div>

      </div>

    </div>
  </form>

    <div id="footer">
